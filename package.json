{
  "name": "shuriken",
  "version": "1.0.0",
  "description": "A production-ready Next.js starter template",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:pretty": "next dev --turbopack 2>&1 | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "type-check": "tsc --noEmit",
    "docker:dev": "docker compose -f infrastructure/docker/docker-compose.dev.yml up",
    "docker:dev:down": "docker compose -f infrastructure/docker/docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose -f infrastructure/docker/docker-compose.dev.yml logs -f",
    "docker:prod": "docker compose -f infrastructure/docker/docker-compose.prod.yml up -d",
    "docker:prod:down": "docker compose -f infrastructure/docker/docker-compose.prod.yml down",
    "docker:prod:logs": "docker compose -f infrastructure/docker/docker-compose.prod.yml logs -f",
    "db:studio": "pnpm --filter database db:studio",
    "db:generate": "pnpm --filter database db:generate",
    "db:migrate": "pnpm --filter database db:migrate",
    "db:push": "pnpm --filter database db:push",
    "db:seed": "pnpm --filter database db:seed",
    "db:check-migration": "tsx scripts/check-schema-migration.ts",
    "prepare": "husky",
    "secrets:decrypt": "export SOPS_AGE_KEY_FILE=.age-key.txt && make decrypt-local",
    "secrets:decrypt:staging": "export SOPS_AGE_KEY_FILE=.age-key.txt && make decrypt-staging",
    "secrets:decrypt:production": "export SOPS_AGE_KEY_FILE=.age-key.txt && make decrypt-production",
    "secrets:edit": "export SOPS_AGE_KEY_FILE=.age-key.txt && make edit-local",
    "secrets:edit:staging": "export SOPS_AGE_KEY_FILE=.age-key.txt && make edit-staging",
    "secrets:edit:production": "export SOPS_AGE_KEY_FILE=.age-key.txt && make edit-production"
  },
  "lint-staged": {
    "*": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.6",
    "@react-email/render": "^2.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^10.3.0",
    "prettier": "^3.8.1",
    "react": "19.2.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "redis": "^5.10.0",
    "resend": "^6.9.1",
    "sonner": "^2.0.7",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.9.19",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "null-loader": "^4.0.1",
    "pino-pretty": "^13.1.3",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
